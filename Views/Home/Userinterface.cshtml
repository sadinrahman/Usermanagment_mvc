@page
@model UserManagementApp.Models.LoginViewModel


<div class="container mt-5">
    <h2>Welcome to your Dashboard, @User.Identity.Name!</h2>
    <p>This is your personalized space where you can manage your account.</p>

    <form asp-action="signout" method="post">
        <input type="submit" name="signout" value="signout" />
    </form>
</div>
